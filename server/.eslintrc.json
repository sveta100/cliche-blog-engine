{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
    "no-underscore-dangle": "off",
    // allow console and debugger in development
    "no-console": 0,
    "no-debugger": 0
    }
}
